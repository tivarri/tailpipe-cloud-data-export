{
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "Service": [
					"billingreports.amazonaws.com",
					"bcm-data-exports.amazonaws.com"
				]
            },
            "Action": [
                "s3:PutObject",
                "s3:GetBucketPolicy"
            ],
            "Resource": [
				"arn:aws:s3:::tailpipe-dataexport-471112783680",
				"arn:aws:s3:::tailpipe-dataexport-471112783680/*"
			],
            "Condition": {
                "StringLike": {
                    "aws:SourceAccount": "471112783680",
                    "aws:SourceArn": [
						"arn:aws:cur:us-east-1:471112783680:definition/*",
						"arn:aws:bcm-data-exports:us-east-1:471112783680:export/*"
					]
                }
            }
        }
    ]
}
